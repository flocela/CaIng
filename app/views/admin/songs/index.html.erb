<h2>Songs</h2>
<%= link_to 'Sign Out', destroy_admin_session_path, method: :delete %>
<%= link_to 'New Song', new_admin_song_path%>
<% @songs.each do |song| %>
  <%= song.new_work_title %>
  <%= link_to 'Delete', "songs/#{song.id}", method: :delete %> 
  <%= link_to 'Edit', "songs/#{song.id}/edit", method: :get %> 
<% end %>

